{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "./dist",
		"noImplicitReturns": false,
		"removeComments": true,
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true   
	},
	"exclude": [
		"node_modules",
		".vscode-test",
		"test",
		"dist"
	],
	"compileOnSave": true
}
